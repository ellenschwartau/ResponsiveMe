/**
 * Enth√§lt die Konfiguration zum Erstellen des JSDoc.
 */
{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": [
      "./background",
      "./contentScripts",
      "./helper",
      "./modules",
      "./popup",
      "./requirejs",
      "./main.js",
      "./config.js"
    ],
    "includePattern": ".+\\.js(doc)?$"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}